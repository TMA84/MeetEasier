{
  "name": "ui-react",
  "version": "1.2.3",
  "private": true,
  "dependencies": {
    "jquery": "^3.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "reload": "^3.4.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^15.10.0",
    "jest-extended": "^4.0.2",
    "jsdom": "^25.0.1",
    "sass": "^1.97.3",
    "sinon": "^19.0.2",
    "start-server-and-test": "^2.1.3",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "test:e2e": "start-server-and-test start http://localhost:5173 cypress:run",
    "test:all": "npm run test:ci && npm run cypress:headless"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/main.jsx",
      "!src/**/*Container.{js,jsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "esbuild": "^0.24.0",
    "diff": "^8.0.3"
  }
}
