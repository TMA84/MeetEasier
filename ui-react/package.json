{
  "name": "ui-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "reload": "^3.4.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "cypress": "^15.10.0",
    "jest-extended": "^4.0.2",
    "sass": "^1.97.3",
    "sinon": "^19.0.2",
    "start-server-and-test": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --env=jsdom --coverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "test:e2e": "start-server-and-test start http://localhost:8080 cypress:run",
    "test:all": "npm run test:ci && npm run cypress:headless",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/**/*Container.{js,jsx}"
    ]
  },
  "proxy": "http://localhost:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.4",
    "resolve-url-loader": "^5.0.0",
    "svgo": "^3.0.0",
    "webpack-dev-server": "^5.2.1",
    "cross-spawn": "^7.0.5",
    "brace-expansion": "^2.0.2",
    "node-forge": "^1.3.2",
    "diff": "^8.0.3"
  }
}
